<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
                              "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config>
 <converter>
  <converter-id>enumConverter</converter-id>
  <converter-class>travel.jsf.converters.EnumConverter</converter-class>
 </converter>
 <managed-bean>
  <managed-bean-name>quarterMain</managed-bean-name>
  <managed-bean-class>travel.jsf.mBeans.bBeans.trip.quarters.QuarterMain</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>facade</property-name>
   <value>#{externalFacade}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>appCtx</managed-bean-name>
  <managed-bean-class>travel.jsf.mBeans.main.AppContext</managed-bean-class>
  <managed-bean-scope>application</managed-bean-scope>
  <managed-property>
   <property-name>imgRes</property-name>
   <property-class>java.lang.String</property-class>
   <value>web/main/images</value>
  </managed-property>
  <managed-property>
   <property-name>cssRes</property-name>
   <property-class>java.lang.String</property-class>
   <value>web/main/css</value>
  </managed-property>
  <managed-property>
   <property-name>mainRes</property-name>
   <property-class>java.lang.String</property-class>
   <value>/web/main</value>
  </managed-property>
  <managed-property>
   <property-name>clientRes</property-name>
   <property-class>java.lang.String</property-class>
   <value>/web/client</value>
  </managed-property>
  <managed-property>
   <property-name>managerRes</property-name>
   <property-class>java.lang.String</property-class>
   <value>/web/manager</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>user</managed-bean-name>
  <managed-bean-class>travel.jsf.mBeans.main.User</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>logged</property-name>
   <property-class>boolean</property-class>
   <value>false</value>
  </managed-property>
  <managed-property>
   <property-name>role</property-name>
   <property-class>java.lang.String</property-class>
   <value>UNKNOWN</value>
  </managed-property>
  <managed-property>
   <property-name>userFacade</property-name>
   <property-class>travel.business.facades.UserFacade</property-class>
   <value>#{userFacade}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>auth</managed-bean-name>
  <managed-bean-class>travel.jsf.mBeans.main.Authentication</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>userFacade</property-name>
   <property-class>travel.business.facades.UserFacade</property-class>
   <value>#{userFacade}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>reg</managed-bean-name>
  <managed-bean-class>travel.jsf.mBeans.bBeans.Registration</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>userFacade</property-name>
   <property-class>travel.business.facades.UserFacade</property-class>
   <value>#{userFacade}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>transm</managed-bean-name>
  <managed-bean-class>travel.jsf.mBeans.bBeans.external.transport.TransportManagement</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>externalFacade</property-name>
   <property-class>travel.business.facades.ExternalFacade</property-class>
   <value>#{externalFacade}</value>
  </managed-property>
 </managed-bean>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>HOME</from-outcome>
   <to-view-id>/web/main/home.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>CLIENT_REGISTER</from-outcome>
   <to-view-id>/web/clients/register.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>WORKER_REGISTER</from-outcome>
   <to-view-id>/web/managers/register.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>TRANSPORT_MANAGE</from-outcome>
   <to-view-id>/web/workers/external/transportsManagement.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/web/clients/register.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>FAILURE</from-outcome>
   <to-view-id>/web/clients/register.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/web/workers/external/transportsManagement.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>EDIT_RECORD</from-outcome>
   <to-view-id>/web/workers/external/transport/editForm.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>NEW_RECORD</from-outcome>
   <to-view-id>/web/workers/external/transport/editForm.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application>
  <locale-config>
   <default-locale>pl</default-locale>
   <supported-locale>pl</supported-locale>
  </locale-config>
  <variable-resolver>org.springframework.web.jsf.DelegatingVariableResolver</variable-resolver>
 </application>
</faces-config>
